<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android" tools:ignore="MissingDefaultResource">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <gradient
                android:angle="60"
                android:endColor="#3BB1C1"
                android:centerColor="#1470BF"
                android:startColor="#028395"
                android:type="linear" />
            <corners android:radius="25dp"/>
            <stroke
                android:width="2dp"
                android:color="@color/white" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient
                android:angle="60"
                android:endColor="#3BB1C1"
                android:centerColor="#1470BF"
                android:startColor="#028395"
                android:type="linear" />
            <corners android:radius="25dp"/>
            <stroke
                android:width="2dp"
                android:color="@color/white" />
        </shape>
    </item>
</selector>